.blog-small-record img {
    vertical-align: top;
}
.blog-small-record {
    padding-left: 0px;
    height: 20px;
}
.blog-small-record li {
    float: left;
    padding: 0 15px 0 0;
}

.blog-small-record ul li {
    left: 0;
    list-style: none outside none;
}
.blog-small-record ul {
    padding: 0;
    margin: 0;
}

.blog-small-record li:first-child {
	margin-left: 0;
	left: 0;
	padding-left: 0;
}

{% if (SC_VERSION < 20) %}


.blog-small-record li {
    float: left;
    padding: 0 18px;
}

.blog-small-record li:first-child {
	padding-left: 18px;
}


ins.ascp_list_grid {
    height: 16px;
    width: 16px;
    line-height: 16px;
    text-indent: 100%;
    white-space: nowrap;
    overflow: hidden;
    font-size: 100%;
    font-weight: normal;
    text-decoration: none;
    float: left;
}
ins.ascp_grid {
    background: url("{{ css_path~'ascp_grid.png' }}") no-repeat 0px 0px transparent;
}
ins.ascp_grid:hover,  ins.ascp_grid_active {
    background: url("{{ css_path~'ascp_grid.png' }}") no-repeat 0px -16px transparent;
}
ins.ascp_list {
    background: url("{{ css_path~'ascp_list.png' }}") no-repeat 0px 0px transparent;
}
ins.ascp_list:hover {
    background: url("{{ css_path~'ascp_list.png' }}") no-repeat 0px -16px transparent;
}
ins.ascp_list_active {
    background: url("{{ css_path~'ascp_list.png' }}") no-repeat 0px -16px transparent;
}
{% endif %}


{% if (SC_VERSION > 15) %}

a #ascp_list:after {
    content: '\f00b\0020';
    font-family: FontAwesome;


}

a #ascp_grid:after {
    content: '\f00a\0020';
    font-family: FontAwesome;

}

#ascp_list, #ascp_grid {
	color: #aaa;

	font-weight: normal;
	font-size: 1.5em;
}


a:hover #ascp_list, a:hover #ascp_grid, #ascp_list.ascp_list_active, #ascp_grid.ascp_grid_active  {
    color: #555;
}


{% endif %}


